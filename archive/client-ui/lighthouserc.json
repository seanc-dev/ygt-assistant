{
  "ci": {
    "collect": {
      "numberOfRuns": 1,
      "startServerCommand": "npm run dev",
      "url": [
        "http://localhost:3000/dashboard",
        "http://localhost:3000/rules",
        "http://localhost:3000/profile"
      ],
      "settings": {
        "preset": "desktop"
      }
    },
    "assert": {
      "assertions": {
        "categories:accessibility": ["error", { "minScore": 0.9 }]
      }
    }
  }
}
